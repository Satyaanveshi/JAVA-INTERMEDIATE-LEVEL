//The program finds all the words in the given
//sentence which are palindromes.

import java.util.*;
class sentencepalinfinder
{
    String n,os="",ns="";
    int len,i;
    void input()
    {
     Scanner sc=new Scanner(System.in);
     System.out.println("Enter a sentence:");
     n=sc.nextLine()+" ";
     len=n.length();
    }
    void palin()
    {
    for(i=0;i<len;i++)
    {
       char ch=n.charAt(i);      
       if(ch!=' ')
       {
           os=os+ch;
           ns=ch+ns;
       }
       else
       {
            if(os.equals(ns))
            System.out.println(os);
            os="";ns="";
       }
    }
    }
    public static void main()
    {
       sentencepalinfinder ob=new sentencepalinfinder();
       ob.input();
       ob.palin();
    }
}
